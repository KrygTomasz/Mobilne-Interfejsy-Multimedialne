<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kryguu.laboratoria8.GestureActivity">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <fragment
            android:name="com.example.kryguu.laboratoria8.InfoFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/doubleTapF"
            tools:layout="@layout/fragment_info"></fragment>
        <fragment
            android:name="com.example.kryguu.laboratoria8.InfoFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/doubleTapEventF"
            tools:layout="@layout/fragment_info"></fragment>
        <fragment
            android:name="com.example.kryguu.laboratoria8.InfoFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/downF"
            tools:layout="@layout/fragment_info"></fragment>
        <fragment
            android:name="com.example.kryguu.laboratoria8.InfoFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/flingF"
            tools:layout="@layout/fragment_info"></fragment>
        <fragment
            android:name="com.example.kryguu.laboratoria8.InfoFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/longPressF"
            tools:layout="@layout/fragment_info"></fragment>
        <fragment
            android:name="com.example.kryguu.laboratoria8.InfoFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/scrollF"
            tools:layout="@layout/fragment_info"></fragment>
        <fragment
            android:name="com.example.kryguu.laboratoria8.InfoFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/showPressF"
            tools:layout="@layout/fragment_info"></fragment>
        <fragment
            android:name="com.example.kryguu.laboratoria8.InfoFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/singleTapConfirmedF"
            tools:layout="@layout/fragment_info"></fragment>
        <fragment
            android:name="com.example.kryguu.laboratoria8.InfoFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/singleTapUpF"
            tools:layout="@layout/fragment_info"></fragment>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
